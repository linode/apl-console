  "version": "0.2.0",
  "configurations": [
    {
      "name": "Reconnect to a Chrome instance with remote debugger plugin enabled",
      "port": 9222,
      "request": "attach",
      "type": "pwa-chrome",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Start a Chrome debugging session",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceRoot}/src"
    },
    {
      "type": "lua",
      "request": "launch",
      "name": "Launch the test lua file",
      "program": "${workspaceFolder}/nginx/session_test.lua"
    }
  ]
}
