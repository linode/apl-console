    "@types/js-yaml": "^4.0.5",
    "@types/json-schema": "^7.0.9",
    "@types/lodash": "^4.14.170",
    "@types/react": "^16.14.23",
    "@types/react-dom": "^16.9.14",
    "@types/react-helmet": "^5.0.15",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "commitizen": "^4.2.3",
    "cspell": "^4.2.8",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.28.0",
    "git-cz": "^4.7.6",
    "husky": "^7.0.2",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react-error-overlay": "6.0.10",
    "standard-version": "^9.1.1",
    "stylelint-config-recommended": "^3.0.0",
    "supports-color": "^9.0.2",
    "ts-jest": "^26.5.4",
    "typescript": "^4.1.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "name": "otomi-console",
  "private": true,
  "resolutions": {},
  "run-if-changed": {
    "package-lock.json": "npm ci --prefer-offline --no-audit"
  },
  "scripts": {
    "build": "PUBLIC_URL='##CONTEXT_PATH##' react-scripts build",
    "cz:retry": "git-cz --retry",
    "cz": "git-cz",
    "dev:docker": "npm ci && npm run dev",
    "dev": "run-p start watch:ts",
    "eject": "react-scripts eject",
    "format:fix": "prettier --write \"**/*.{js,json,md,ts,yml,yaml}\"",
    "format": "prettier --check \"**/*.{js,json,md,ts,yml,yaml}\"",
    "lint-staged": "lint-staged",
    "lint:es": "eslint --ext js,ts,tsx .",
    "lint:fix": "eslint --ext js,ts,tsx --fix .",
    "lint": "run-p types lint:es spellcheck",
    "//preinstall": "npx npm-force-resolutions",
    "prepare": "husky install",
    "release": "standard-version",
    "run-if-changed": "run-if-changed",
    "spellcheck": "cspell 'public/i18n/**/*.json'",
    "start": "dotenv -e .env react-scripts start --no-cache",
    "test:enzyme": "react-scripts test",
    "types": "tsc --noEmit",
    "watch:ts": "tsc -w --noEmit"
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  },
  "version": "0.4.71"
}
