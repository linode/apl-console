{
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@cypress/browserify-preprocessor": "^3.0.0",
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@hkdobrev/run-if-changed": "^0.3.1",
    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.9.6",
    "@percy/cypress": "^2.3.1",
    "@rjsf/core": "^2.0.0-alpha.6",
    "@rjsf/material-ui": "^2.0.0-alpha.4",
    "@types/chai": "^4.2.11",
    "@types/cucumber": "^6.0.1",
    "@types/cypress-cucumber-preprocessor": "^1.14.1",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^24.9.1",
    "@types/material-ui": "^0.21.7",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "clsx": "^1.1.0",
    "commitizen": "^4.0.3",
    "cypress": "^4.8.0",
    "cypress-cucumber-preprocessor": "^2.4.0",
    "cypress-terminal-report": "^1.4.0",
    "cz-conventional-changelog": "^3.1.0",
    "debug": "^4.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-unused-imports": "^0.1.2",
    "http-proxy-middleware": "^0.20.0",
    "husky": "^3.1.0",
    "i18next": "^19.4.4",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^9.5.0",
    "lodash": "^4.17.15",
    "lodash.isnil": "^4.0.0",
    "lodash.omitby": "^4.6.0",
    "notistack": "^0.9.9",
    "npm-force-resolutions": "0.0.3",
    "npm-run-all": "^4.1.5",
    "openapi-client-axios": "^3.3.2",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.0-beta.17",
    "react-bootstrap-table-next": "^4.0.0",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-i18next": "^11.5.0",
    "react-icons": "^3.9.0",
    "react-jsonschema-form": "^1.8.1",
    "react-redux-typescript-scripts": "^1.6.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.2.0",
    "react-test-renderer": "^16.13.1",
    "standard-version": "^7.1.0",
    "ts-jest": "^24.3.0",
    "tsify": "^4.0.2",
    "typeface-roboto": "0.0.75",
    "typescript": "^3.8.3",
    "yup": "^0.27.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-checkout": "run-if-changed",
      "post-commit": "run-if-changed",
      "post-merge": "run-if-changed",
      "post-rewrite": "run-if-changed",
      "pre-commit": "npm run husky:pre-commit"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --ext js,ts,tsx"
    ]
  },
  "name": "otomi-stack-web",
  "private": true,
  "run-if-changed": {
    "package-lock.json": "npm install --prefer-offline --no-audit"
  },
  "scripts": {
    "build": "react-scripts build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cz": "git-cz",
    "cz:retry": "git-cz --retry",
    "dev": "run-p start watch:ts",
    "dev:docker": "npm install && npm run dev",
    "e2e": "percy exec -- cypress run",
    "eject": "react-scripts eject",
    "format": "prettier --check \"**/*.{js,json,md,ts,yml,yaml}\"",
    "format:fix": "prettier --write \"**/*.{js,json,md,ts,yml,yaml}\"",
    "husky:lint-staged": "lint-staged",
    "husky:pre-commit": "tsc --noEmit && lint-staged",
    "lint": "tsc --noEmit && eslint .",
    "lint:fix": "eslint --fix .",
    "release": "standard-version",
    "start": "react-scripts start --no-cache",
    "test:enzyme": "react-scripts test",
    "types": "tsc --noEmit",
    "types:cypress": "tsc -p cypress/tsconfig.json --noEmit",
    "watch:ts": "tsc -w --noEmit",
    "watch:ts:cypress": "tsc -w -p cypress/tsconfig.json --noEmit"
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  },
  "version": "0.2.38"
}
