{
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@hkdobrev/run-if-changed": "^0.3.1",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@material-ui/pickers": "^3.3.10",
    "@material-ui/styles": "^4.11.3",
    "@redkubes/otomi-api-client-axios": "^0.4.49",
    "@rjsf/core": "^2.5.1",
    "@rjsf/material-ui": "^2.5.1",
    "@types/chai": "^4.2.15",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.168",
    "@types/material-ui": "^0.21.7",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.12",
    "@types/react-helmet": "^5.0.15",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "clsx": "^1.1.0",
    "commitizen": "^4.2.3",
    "cspell": "^4.2.8",
    "cz-conventional-changelog": "^3.3.0",
    "debug": "^4.3.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-unused-imports": "^0.1.2",
    "http-proxy-middleware": "^0.20.0",
    "husky": "^3.1.0",
    "i18next": "^19.9.2",
    "i18next-browser-languagedetector": "^6.1.0",
    "i18next-http-backend": "^1.2.1",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^9.5.0",
    "lodash": "^4.17.21",
    "lodash.isnil": "^4.0.0",
    "lodash.omitby": "^4.6.0",
    "notistack": "^0.9.9",
    "npm-force-resolutions": "0.0.3",
    "npm-run-all": "^4.1.5",
    "openapi-client-axios": "^3.13.1",
    "prettier": "^2.2.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.5.2",
    "react-bootstrap-table-next": "^4.0.3",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-i18next": "^11.8.11",
    "react-icons": "^3.9.0",
    "react-redux-typescript-scripts": "^1.6.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.3",
    "react-test-renderer": "^16.13.1",
    "standard-version": "^9.1.1",
    "stylelint-config-recommended": "^3.0.0",
    "ts-jest": "^26.5.4",
    "typeface-comfortaa": "^1.1.13",
    "typeface-roboto": "0.0.75",
    "typescript": "^4.1.2",
    "yup": "^0.27.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-checkout": "run-if-changed",
      "post-commit": "run-if-changed",
      "post-merge": "run-if-changed",
      "post-rewrite": "run-if-changed",
      "pre-commit": "npm run husky:pre-commit"
    }
  },
  "lint-staged": {
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.{js,ts,tsx}": [
      "eslint --ext js,ts,tsx --fix"
    ]
  },
  "name": "otomi-console",
  "private": true,
  "run-if-changed": {
    "package-lock.json": "npm install --prefer-offline --no-audit"
  },
  "scripts": {
    "build": "PUBLIC_URL='##CONTEXT_PATH##' react-scripts build",
    "cz": "git-cz",
    "cz:retry": "git-cz --retry",
    "dev": "run-p start watch:ts",
    "dev:docker": "npm install && npm run dev",
    "eject": "react-scripts eject",
    "format": "prettier --check \"**/*.{js,json,md,ts,yml,yaml}\"",
    "format:fix": "prettier --write \"**/*.{js,json,md,ts,yml,yaml}\"",
    "husky:lint-staged": "lint-staged",
    "husky:pre-commit": "lint-staged && npm run lint",
    "lint": "run-p types lint:es spellcheck",
    "lint:es": "eslint --ext js,ts,tsx .",
    "lint:fix": "eslint --ext js,ts,tsx --fix .",
    "release": "standard-version",
    "spellcheck": "cspell 'public/i18n/**/*.json'",
    "start": "react-scripts start --no-cache",
    "test:enzyme": "react-scripts test",
    "types": "tsc --noEmit",
    "watch:ts": "tsc -w --noEmit"
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  },
  "version": "0.4.45"
}
