  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@hkdobrev/run-if-changed": "0.3.1",
    "@mui/types": "7.1.2",
    "@rtk-query/codegen-openapi": "1.0.0-alpha.1",
    "@types/enzyme": "3.10.11",
    "@types/jest": "27.4.1",
    "@types/js-yaml": "4.0.5",
    "@types/json-schema": "7.0.9",
    "@types/lodash": "4.14.181",
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.1",
    "@types/react-helmet": "6.1.5",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.7",
    "commitizen": "4.2.4",
    "cspell": "5.19.5",
    "cz-conventional-changelog": "3.3.0",
    "enzyme": "3.11.0",
    "eslint": "8.9.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.4",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "git-cz": "4.8.0",
    "husky": "7.0.4",
    "jest-enzyme": "7.1.2",
    "lint-staged": "12.3.8",
    "node-fetch": "3.2.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "react-error-overlay": "6.0.10",
    "redux-logger": "3.0.6",
    "sleep-promise": "9.1.0",
    "standard-version": "9.3.2",
    "stylelint-config-recommended": "7.0.0",
    "supports-color": "9.2.1",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "typescript": "4.5.5"
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "name": "otomi-console",
  "private": true,
  "resolutions": {},
  "run-if-changed": {
    "package-lock.json": "npm ci --prefer-offline --no-audit"
  },
  "scripts": {
    "build": "PUBLIC_URL='##CONTEXT_PATH##' react-scripts build",
    "cz:retry": "git-cz --retry",
    "cz": "git-cz",
    "dev:docker": "npm ci && npm run dev",
    "dev": "run-p start watch:ts",
    "eject": "react-scripts eject",
    "format:fix": "prettier --write \"**/*.{js,json,md,ts,yml,yaml}\"",
    "format": "prettier --check \"**/*.{js,json,md,ts,yml,yaml}\"",
    "gen:store": "npx @rtk-query/codegen-openapi src/redux/openapi-config.ts",
    "lint-staged": "lint-staged",
    "lint:es": "eslint --ext js,ts,tsx .",
    "lint:fix": "eslint --ext js,ts,tsx --fix .",
    "lint": "run-p types lint:es",
    "//preinstall": "npx npm-force-resolutions",
    "prepare": "husky install",
    "release": "standard-version",
    "run-if-changed": "run-if-changed",
    "spellcheck": "cspell 'public/i18n/**/*.json'",
    "start": "dotenv -e .env react-scripts start --no-cache",
    "lang:server": "DEBUG='*' node --no-warnings --experimental-json-modules --loader ts-node/esm src/lang-server",
    "lang:export": "node --nolazy --loader ts-node/esm src/lang-export",
    "lang:import": "IMPORT=1 node --nolazy --loader ts-node/esm src/lang-export",
    "test": "react-scripts test",
    "types": "tsc --noEmit",
    "watch:ts": "tsc -w --noEmit"
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  },
  "version": "0.5.6"
}
